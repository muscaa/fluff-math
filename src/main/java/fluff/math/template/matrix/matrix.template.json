{
	"include": [
		"INumberMatrix.java",
		"AbstractNumberMatrix2.java",
		"AbstractNumberMatrix3.java",
		"Mat2n.java",
		"Mat3n.java"
	],
	"generate": [
		{
			"from": "number",
			"to": [
				"int",
				"long",
				"float",
				"double"
			]
		}
	],
	"define": [
		{
			"from": "public",
			"to": "public"
		}
	],
	"define_if": [
		[
			"long_name",
			"short_name",
			"convert_ij",
			"convert_ijk"
		],
		{
			"if": "/*number*/",
			"equals": "int",
			"then": [
				"Int",
				"i",
				"+ get(i, j)",
				"+ get(i, j, k)"
			]
		},
		{
			"if": "/*number*/",
			"equals": "long",
			"then": [
				"Long",
				"l",
				"+ get(i, j)",
				"+ get(i, j, k)"
			]
		},
		{
			"if": "/*number*/",
			"equals": "float",
			"then": [
				"Float",
				"f",
				"+ Float.floatToIntBits(get(i, j))",
				"+ Float.floatToIntBits(get(i, j, k))"
			]
		},
		{
			"if": "/*number*/",
			"equals": "double",
			"then": [
				"Double",
				"d",
				"+ Double.doubleToLongBits(get(i, j))",
				"+ Double.doubleToLongBits(get(i, j, k))"
			]
		}
	],
	"replace": [
		{
			"from": "fluff.math.template",
			"to": "fluff.math.gen._/*number*/"
		},
		{
			"from": "INumberMatrix",
			"to": "I/*long_name*/Matrix"
		},
		{
			"from": "AbstractNumberMatrix2",
			"to": "Abstract/*long_name*/Matrix2"
		},
		{
			"from": "AbstractNumberMatrix3",
			"to": "Abstract/*long_name*/Matrix3"
		},
		{
			"from": "Mat2n",
			"to": "Mat2/*short_name*/"
		},
		{
			"from": "Mat3n",
			"to": "Mat3/*short_name*/"
		}
	],
	"destination": "gen/_/*number*//matrix"
}