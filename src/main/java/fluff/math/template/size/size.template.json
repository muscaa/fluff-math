{
	"include": [
		"INumberSize.java",
		"Size2n.java",
		"Size3n.java"
	],
	"generate": [
		{
			"from": "number",
			"to": [
				"int",
				"long",
				"float",
				"double"
			]
		}
	],
	"define": [
		{
			"from": "public",
			"to": "public"
		}
	],
	"define_if": [
		[
			"long_name",
			"short_name"
		],
		{
			"if": "/*number*/",
			"equals": "int",
			"then": [
				"Int",
				"i"
			]
		},
		{
			"if": "/*number*/",
			"equals": "long",
			"then": [
				"Long",
				"l"
			]
		},
		{
			"if": "/*number*/",
			"equals": "float",
			"then": [
				"Float",
				"f"
			]
		},
		{
			"if": "/*number*/",
			"equals": "double",
			"then": [
				"Double",
				"d"
			]
		}
	],
	"replace": [
		{
			"from": "fluff.math.template",
			"to": "fluff.math.gen._/*number*/"
		},
		{
			"from": "INumberSize",
			"to": "I/*long_name*/Size"
		},
		{
			"from": "AbstractNumberVec2",
			"to": "Abstract/*long_name*/Vec2"
		},
		{
			"from": "AbstractNumberVec3",
			"to": "Abstract/*long_name*/Vec3"
		},
		{
			"from": "Size2n",
			"to": "Size2/*short_name*/"
		},
		{
			"from": "Size3n",
			"to": "Size3/*short_name*/"
		}
	],
	"destination": "gen/_/*number*//size"
}